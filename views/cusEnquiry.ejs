  <script>
    function pageClick5(pageNumber) {
      document.getElementById("page-number-5").innerText = pageNumber;
    }
    document.addEventListener("DOMContentLoaded", function() {

      var itemsOnPage = 5;
      var itemsCount = $('#totalData').val();

      function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
          results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
      }

      var pagination5 = new Pagination({
        container: document.getElementById("pagination-5"),
        pageClickUrl: function(num) {
          return "?page=" + num;
        },
        pageClickCallback: pageClick5,
        callPageClickCallbackOnInit: true,
        // enhancedMode: true,
        maxVisibleElements: 6
      });
      pagination5.make(itemsCount, itemsOnPage, getParameterByName("page"));


    });
  </script>


  <script>
    function updateEnq(e) {
      let enqid = e.getAttribute('data-id');
      // alert(id)
      $.ajax({
        url: '/updateEnq',
        method: "POST",
        data: {
          _id: enqid
        },
        success: (res) => {
          if (res == "Done") {
            iziToast.warning({
              title: 'Error',
              message: 'Already Updated!',
            });

          } else if (res == "Updated") {
            iziToast.success({
              title: 'OK',
              message: 'Successfully Updated!',
            });
            setTimeout(() => {
              location.reload();
            }, 3000)
          }
        }
      })
    }
  </script>


