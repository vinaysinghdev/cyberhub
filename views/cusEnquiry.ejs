            <div class="card-header pb-0 pt-3 bg-transparent d-flex justify-content-between">
              <h6>Enquiry table</h6>
              <h6 class="text-capitalize">( <%= total %> )</h6>
            </div>
            <div class="card-body px-0 pt-0 pb-2">
              <div class="table-responsive p-0">
                <table class="table align-items-center mb-0">
                  <thead>
                    <tr>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Name</th>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 text-start">Mobile</th>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 text-start">Subject</th>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Message</th>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 ps-3">Enq. Date</th>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Status</th>
                      <th class="text-secondary opacity-7"></th>
                    </tr>
                  </thead>
                  <tbody>

                    <% enqData.forEach((value,index)=>{ %>

                    <tr>
                      <td>
                        <div class="d-flex px-2 py-1">
                          <div>
                            <img src="images/<%=value.profile%>" class="avatar avatar-sm me-3" alt="user1">
                          </div>
                          <div class="d-flex flex-column justify-content-center">
                            <h6 class="mb-0 text-sm"><%= value.name %></h6>
                          </div>
                        </div>
                      </td>
                      <td>
                        <p class="text-xs font-weight-bold mb-0"><%=value.mobile%></p>
                      </td>
                      <td>
                        <p class="text-xs font-weight-bold mb-0"><%=value.subject%></p>
                      </td>
                      <td>
                        <p class="text-xs text-secondary mb-0"><%=value.message%></p>
                      </td>
                      <td class="align-middle text-sm">
                        <span class="text-secondary mx-2 text-xs font-weight-bold"><%=value.date%></span>
                      </td>
                      <td class="align-middle">
                        <% if(value.status == "Done"){ %>
                        <span class="badge mx-2 badge-sm bg-gradient-success">Done</span>

                        <% } else { %>
                        <span class="badge mx-2 badge-sm bg-gradient-danger">Pending</span>
                        <%}%>


                        </td>
                        <td class="align-middle">
                          <a class="text-secondary font-weight-bold text-xs" style="cursor: pointer;" data-id="<%=value._id%>" onclick="updateEnq(this)">
                        Done
                        </a>
                      </td>
                    </tr>
                    <% }) %>
                  </tbody>
                </table>
              </div>

              <div id="content" class="text-center my-2">
                <div class="">
                  <samp class="d-none">
                    <span>Current page:</span>
                    <span id="page-number-5"></span>
                    <input type="number" value="<%=total%>" id="totalData">
                  </samp>
                  <div id="pagination-5"></div>
                </div>
              </div>

            </div>
          </div>
        </div>
      </div>
